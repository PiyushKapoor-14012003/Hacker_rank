//Sum and difference of integer and float numbers
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main()
{
    int n1,n2,n3,n4;
	  float a,b,s,d;
    scanf("%d %d ",&n1,&n2);
    scanf("%f %f ",&a,&b);
    n3=n1+n2;
    n4=n1-n2;
    s=a+b;
    d=a-b;
    printf("%d %d \n",n3,n4);
    printf("%.1f %.1f ",s,d);  
    return 0;
}
