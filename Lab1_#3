//Temperature conversion from fahrenheit to celsius
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    float f,c;
    scanf("%f ",&f);
    c=5*(f-32)/9;
    printf("%.2f ",c);
    return 0;
}
