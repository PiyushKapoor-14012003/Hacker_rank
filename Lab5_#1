#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int num1,num2,result=0;
    char operator;
    scanf("%d %d\n",&num1,&num2);
    scanf("%c \n",&operator);
    switch(operator)
    {
        case '+':
            result=num1+num2;
            printf("%d ",result);
            break;
        case '-':
            result=num1-num2;
            printf("%d ",result);
            break;
        case '/':
            result=num1/num2;
            printf("%d ",result);
            break;
        case '*':
            result=num1*num2;
            printf("%d ",result);
            break;
        default:
            printf("Invalid Choice");
            break;
    }
    return 0;
}
