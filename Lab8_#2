#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,i,j,sum;
    scanf("%d ",&n);
        sum=0;
        for(j=1;j<n;j++)
            if(n%j==0)
                sum=sum+j;
        if(sum==n)
            printf("YES");
    else
        printf("NO");
        
    return 0;
}
